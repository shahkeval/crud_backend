{
    "version": 2,
    "builds": [
      {
        "src": "server.js", 
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "server.js"
      }
    ],
    "headers": [
        {
          "key": "Content-Security-Policy",
          "value": "default-src 'crud-backend-delta.vercel.app'; script-src 'crud-backend-delta.vercel.app' https://vercel.live;"
        }
    ]
  }